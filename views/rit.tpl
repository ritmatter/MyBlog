<h2>Login</h2>

<p>Enter your username and password if you dare!  Trespassers will be killed on sight!</p>

<form action="/rit" method="post">
	<div>
		<input name="username" type="text" placeholder="Username" maxlength="100" size="30"/>
		<input name="password" type="password" placeholder="Password" maxlength="30" size="30"/>
	</div>
	<div>
		<input type="submit" name="submit" value="Login" />
	</div>
</form>
%rebase base
